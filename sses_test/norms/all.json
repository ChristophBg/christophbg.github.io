total <- read.csv("sses_reference.csv")$total  # adapt
stopifnot(all(total >= 10 & total <= 50))

d <- density(total, from = 10, to = 50, n = 401)  # KDE grid
out <- list(
  n = length(total),
  kde = list(x = unname(d$x), y = unname(d$y))
)

jsonlite::write_json(out, "all.json", auto_unbox = TRUE, digits = 10)

